
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
<h2>Channel Details</h2>

<table>
    <tr>
        <th>Channel ID</th>
        <td>@Model.ChannelId</td>
    </tr>
    <tr>
        <th>Channel Name</th>
        <td>@Model.ChannelName</td>
    </tr>
    <tr>
        <th>Established Year</th>
        <td>@Model.EstablishedYear</td>
    </tr>
    <tr>
        <th>Country</th>
        <td>@Model.Country</td>
    </tr>
</table>

<a href="@Url.Action("List", "Channel")">Back to List</a>
